grammar BibTex

	rule bib
		.* book* .* {
		def label
			return "bib"
		end
		}
	end

	rule book
		title:bookTitle '{' space key:Character+ space ',' (attribute space ',' space)+ attribute '}' .* {
			def label
				return "book"
			end
		}
	end

	rule attribute
		Character+ space '=' space value
	end

	rule value
		('"' .* '"') / ('{' .* '}')
	end

	rule bookTitle
		'@'  Character+
	end

	rule space
		' '* {
			def label
				return "white"
			end
		}
	end

end